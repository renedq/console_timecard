<div class='page-header'>
  <h2><%= @unit.name %></h2> 
  <h4><%= @unit.city %>, <%= @unit.state %></h4> 
  <h4><%= @unit.phone_number %></h4> 
  <center><h2>
  <% if @earlier %>
    <%= link_to '<<', timecards_path(FY: (@fy - 1.year).year) %> 
  <% end %>
  <%= @fy.year %> 
  <% if @later %>
    <%= link_to '>>', timecards_path(FY: (@fy + 1.year).year) %>
  <% end %>
  </h2></center>
	
	<table class='table table-striped table-hover' width=60%>
		<thead>
			<tr>
				<th>Name</th>
				<th>Oct</th>
				<th>Nov</th>
				<th>Dec</th>
				<th>Jan</th>
				<th>Feb</th>
				<th>Mar</th>
				<th>Apr</th>
				<th>May</th>
				<th>Jun</th>
				<th>Jul</th>
				<th>Aug</th>
				<th>Sep</th>
				<th>FY</th>
			</tr>
		</thead>

		<tbody>
			<% @users_data.each do |user| %>
				<tr>
					<td><%= link_to user['user']['last_name'] + ", " + user['user']['first_name'], user_path(id: user['user']['id']), method: 'GET' %></td>
					<td><%= user['oct'] %></td>
					<td><%= user['nov'] %></td>
					<td><%= user['dec'] %></td>
					<td><%= user['jan'] %></td>
					<td><%= user['feb'] %></td>
					<td><%= user['mar'] %></td>
					<td><%= user['apr'] %></td>
					<td><%= user['may'] %></td>
					<td><%= user['jun'] %></td>
					<td><%= user['jul'] %></td>
					<td><%= user['aug'] %></td>
					<td><%= user['sep'] %></td>
					<td><%= user['fy'] %></td>
				</tr>
			<% end %>
		</tbody>
	</table>
</div>
