<div class='page-header'>
    <table class='table table-striped table-hover'>
      <thead>
        <tr>
          <th><br></th>
          <th>Name</th>
          <th><%= DateTime.now.utc.strftime("%B") %></th>
          <th>Quarter</th>
          <th>FY</th>
        </tr>
      </thead>

      <tbody>
        <% @users_data.each do |user| %>
          <tr>
            <% 
            if user['user'].timecards.order(start_time: :desc).first.hours != nil %>
            <td><%= link_to 'Punch In', timecards_start_path(id: user['user']['id']), method: 'POST' %></td>
          <% else %>
            <td><%= link_to 'Punch Out', timecards_finish_path(id: user['user'].timecards.where(hours: nil).first.id), method: 'POST' %></td>
          <% end %>
            <td><%= link_to user['user']['last_name'] + ", " + user['user']['first_name'], user_path(id: user['user']['id']), method: 'GET' %></td>
            <td><%= user['month'] %></td>
            <td><%= user['quarter'] %></td>
            <td><%= user['fy'] %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
</div>
